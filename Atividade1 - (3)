n1 = int (input("Digite o primeiro numéro: "))
n2 = int (input("Digite o segundo numéro: "))
n3 = int (input("Digite o terceiro numero: "))

if (n1<n2):
   if (n1<n3):
      print ("N1 é o numero menor")
   else:
      print ("N3 é o numero menor)
elif (n2<n3):
      print ("N2 é o numero menor)
else:
      print ("N3 é o numero menor")
      
