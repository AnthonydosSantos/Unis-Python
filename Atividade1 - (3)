n1 = int(input("Digite o primeiro numéro: "))
n2 = int(input("Digite o segundo numéro: "))
n3 = int(input("Digite o terceiro numero: "))

if (n1 < n2):
    if (n1 < n3):
        print("primeiro é o numero menor")
    else:
        print("terceiro é o numero menor")
elif (n2 < n3):
     print("segundo é o numero menor")
else:
     print("terceiro é o numero menor")
